        </shadow>
      </value>
      <value name="position">
        <shadow type="def_position">
          <value name="x">
            <shadow type="math_number">
              <field name="NUM">0</field>
            </shadow>
          </value>
          <value name="y">
            <shadow type="math_number">
              <field name="NUM">0</field>
            </shadow>
          </value>
          <value name="z">
            <shadow type="math_number">
              <field name="NUM">0</field>
            </shadow>
          </value>
        </shadow>
